<div id="filterrific_results">
  <div>
    <%= page_entries_info links %>
  </div>

  <table>
    <tr>
      <th>URL</th>
      <th>Title</th>
      <th>&nbsp;</th>
    </tr>
    <% links.each do |link| %>
      <tr>
        <td><%= link.url %></td>
        <td><%= link.title %></td>
        <td><%= link.read %></td>
      </tr>
    <% end %>
  </table>
</div>

<%= will_paginate links %>
