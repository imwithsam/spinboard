<% if session[:user_id] %>
  <%= link_to "Links", links_path, class: "btn btn-default" %>
  <%= link_to "Sign Out", "/logout", class: "btn btn-default" %>
<% else %>
  <%= link_to "Log In", "/login", class: "btn btn-primary" %>
  <%= link_to "Sign Up", "/signup", class: "btn btn-default" %>
<% end %>
